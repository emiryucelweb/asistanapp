{
  "name": "@asistan/panel",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:analyze": "ANALYZE=true npm run build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:coverage:quick": "vitest run --coverage --reporter=text-summary",
    "test:coverage:html": "vitest run --coverage --reporter=html",
    "test:coverage:ci": "CI=true vitest run --coverage --silent",
    "test:memory-safe": "NODE_OPTIONS='--max-old-space-size=2048' vitest run --no-file-parallelism --pool=forks --poolOptions.forks.singleFork",
    "test:debug-memory": "NODE_OPTIONS='--expose-gc --max-old-space-size=2048' vitest run --inspect-brk",
    "coverage:open": "open coverage/index.html || xdg-open coverage/index.html",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:a11y": "playwright test tests/e2e/accessibility.spec.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lighthouse": "lhci autorun",
    "lighthouse:desktop": "lighthouse http://localhost:4173 --preset=desktop --view",
    "axe": "axe http://localhost:4173 --stdout --exit",
    "analyze": "ANALYZE=true vite build",
    "quality": "npm run type-check && npm run lint && npm run test:coverage && npm run test:e2e && npm run lighthouse",
    "check:i18n": "node scripts/check-i18n.cjs",
    "check:i18n:fix": "node scripts/check-i18n.cjs --fix",
    "chromatic": "npx chromatic --project-token=${CHROMATIC_PROJECT_TOKEN}"
  },
  "dependencies": {
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@headlessui/react": "^1.7.17",
