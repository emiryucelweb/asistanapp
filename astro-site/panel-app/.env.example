# ASISTAN PANEL - ENVIRONMENT VARIABLES

# APPLICATION
VITE_APP_NAME=Asistan Panel
VITE_APP_VERSION=1.0.0
VITE_ENV=development
VITE_SERVICE_NAME=asistan-panel

# API
VITE_API_BASE_URL=http://localhost:3000/api
